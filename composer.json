{
  "name": "inrage/acorn-mail",
  "type": "package",
  "description": "Simple WordPress SMTP using Acorn.",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal GAULT",
      "email": "pascal@inrage.fr",
      "homepage": "https://www.inrage.fr"
    }
  ],
  "require": {
    "php": ">=8.3"
